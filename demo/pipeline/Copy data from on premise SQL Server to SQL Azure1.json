{
	"name": "Copy data from on premise SQL Server to SQL Azure1",
	"properties": {
		"description": "Copy data from on premise SQL Server to SQL Azure.\n\nYou can also go to \"Copy Data Tool\" to get the pipeline for more connectors and scenarios.",
		"activities": [
			{
				"name": "SQL_to_SQLAzure",
				"description": "Copy data from SQL server to SQL Azure",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlSource"
					},
					"sink": {
						"type": "SqlSink",
						"writeBatchSize": 10000
					},
					"enableStaging": false,
					"dataIntegrationUnits": 0,
					"translator": {
						"type": "TabularTranslator",
						"columnMappings": {
							"SalesKey": "SalesKey",
							"DateKey": "DateKey",
							"channelKey": "channelKey",
							"StoreKey": "StoreKey",
							"ProductKey": "ProductKey",
							"PromotionKey": "PromotionKey",
							"CurrencyKey": "CurrencyKey",
							"UnitCost": "UnitCost",
							"UnitPrice": "UnitPrice",
							"SalesQuantity": "SalesQuantity",
							"ReturnQuantity": "ReturnQuantity",
							"ReturnAmount": "ReturnAmount",
							"DiscountQuantity": "DiscountQuantity",
							"DiscountAmount": "DiscountAmount",
							"TotalCost": "TotalCost",
							"SalesAmount": "SalesAmount",
							"ETLLoadID": "ETLLoadID",
							"LoadDate": "LoadDate",
							"UpdateDate": "UpdateDate"
						}
					}
				},
				"inputs": [
					{
						"referenceName": "SQLServer",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "SQLDB",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}